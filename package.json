  "engines": {
    "node": ">=24.12.0"
  },
  "scripts": {
    "build": "nest build",
    "prettier": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "prettier:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "docs": "npx @techdocs/cli serve:mkdocs --verbose",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:mdlint": "markdownlint-cli2 \"**/*.{md,mdx}\" \"!**/node_modules\"",
    "lint:mdlint:fix": "markdownlint-cli2-fix \"**/*.{md,mdx}\" \"!**/node_modules\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/swagger": "^11.2.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.3",
    "@opentelemetry/core": "^2.3.0",
    "@opentelemetry/exporter-jaeger": "^2.3.0",
    "@opentelemetry/exporter-prometheus": "^0.209.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.209.0",
    "@opentelemetry/sdk-node": "^0.209.0",
    "axios": "^1.13.2",
    "pino": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@github/markdownlint-github": "^0.8.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@types/express": "^5.0.6",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "30.2.0",
    "markdownlint-cli2": "^0.20.0",
    "markdownlint-cli2-formatter-pretty": "^0.0.9",
    "prettier": "^3.7.4",
    "prettier-eslint": "^16.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
